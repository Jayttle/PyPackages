{
	"QCustomSlideMenu": [{
		"name": "side_menu",
		"defaultSize": [{
			"width": "150",
			"height": "auto"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": 0
		}],
		"expandedSize": [{
			"width": "150",
			"height": "auto"
		}],
		"toggleButton": [{
			"buttonName": "menuBtn",
			"icons": [{
				"whenMenuIsCollapsed": ":/feather_fff/feather/align-justify.svg",
				"whenMenuIsExpanded": ":/feather_fff/feather/bold.svg"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 2000,
			"animationEasingCurve": "OutBack"
		}]
	}]
}